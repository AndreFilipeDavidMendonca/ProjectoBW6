<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<xsdd:schema xmlns="http://www.w3.org/2001/XMLSchema" 
	targetNamespace="http://www.example.org/Logging" 
	xmlns:xsdd="http://www.w3.org/2001/XMLSchema"
	elementFormDefault="qualified"
	xmlns:finalize="http://www.example.org/Finalize">
	
<xsd:import schemaLocation="Finalize.xsdd" namespace="http://www.example.org/Finalize" />
	
	<xsd:element name="Data">
		
        
        
        
        <xsd:complexType>
			<xsd:sequence>
				<xsd:element minOccurs="0" name="In" type="finalize:In">
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
    </xsd:element>
	<xsd:complexType name="FaultDetails">
		<xsd:sequence>
			<xsd:element name="ActivityName" type="xsdd:string"></xsd:element>
			<xsd:element name="Msg" type="xsdd:string"></xsd:element>
			<xsd:element name="MsgCode" type="xsdd:string"></xsd:element>		
			<xsd:element name="FullClass" type="xsdd:string"></xsd:element>
			<xsd:element name="Class" type="xsdd:string"></xsd:element>
			<xsd:element name="ProcessStack" type="xsdd:string"></xsd:element>
			<xsd:element name="StackTrace" type="xsdd:string"></xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ProcessContext">
		<xsd:sequence>
			<xsd:element name="BusinessDomain" type="xsd:string" minOccurs="0"/>
			<xsd:element name="TechnicalDomain" type="xsd:string" minOccurs="0"/>
			<xsd:element name="BusinessConcept" type="xsd:string" minOccurs="0"/>
			<xsd:element name="BusinessOperation" type="xsd:string" minOccurs="0"/>
			<xsd:element name="BusinessRefs" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element ref="Parameter" minOccurs="0" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="TransactionID" type="xsd:string"/>
			<xsd:element name="ServiceName" type="xsd:string" minOccurs="0"/>
			<xsd:element name="ProcessName" type="xsd:string" minOccurs="0"/>
			<xsd:element name="ComponentName" type="xsd:string" minOccurs="0"/>
			<xsd:element name="HostName" type="xsd:string" minOccurs="0"/>
			<xsd:element name="DateTime" type="xsd:dateTime" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
<xsd:schema></xsd:schema>