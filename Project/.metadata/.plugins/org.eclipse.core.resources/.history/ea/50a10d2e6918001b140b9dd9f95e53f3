<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<xsd:schema xmlns="http://www.w3.org/2001/XMLSchema" 
	targetNamespace="http://www.example.org/Logging" 
	xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	elementFormDefault="qualified"
	xmlns:finalize="http://www.example.org/Finalize">
	
<xsd:import schemaLocation="Finalize.xsd" namespace="http://www.example.org/Finalize"/>
	
	<xsd:element name="Data">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element minOccurs="0" name="In" type="finalize:In">
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:complexType name="FaultDetails">
		<xsd:sequence>
			<xsd:element name="ActivityName" type="xsd:string"></xsd:element>
			<xsd:element name="Msg" type="xsd:string"></xsd:element>
			<xsd:element name="MsgCode" type="xsd:string"></xsd:element>		
			<xsd:element name="FullClass" type="xsd:string"></xsd:element>
			<xsd:element name="Class" type="xsd:string"></xsd:element>
			<xsd:element name="ProcessStack" type="xsd:string"></xsd:element>
			<xsd:element name="StackTrace" type="xsd:string"></xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xs:complexType name="ProcessContext">
		<xs:sequence>
			<xs:element name="BusinessDomain" type="xs:string" minOccurs="0"/>
			<xs:element name="TechnicalDomain" type="xs:string" minOccurs="0"/>
			<xs:element name="BusinessConcept" type="xs:string" minOccurs="0"/>
			<xs:element name="BusinessOperation" type="xs:string" minOccurs="0"/>
			<xs:element name="BusinessRefs" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="Parameter" minOccurs="0" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="TransactionID" type="xs:string"/>
			<xs:element name="ServiceName" type="xs:string" minOccurs="0"/>
			<xs:element name="ProcessName" type="xs:string" minOccurs="0"/>
			<xs:element name="ComponentName" type="xs:string" minOccurs="0"/>
			<xs:element name="HostName" type="xs:string" minOccurs="0"/>
			<xs:element name="DateTime" type="xs:dateTime" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
</xsd:schema>