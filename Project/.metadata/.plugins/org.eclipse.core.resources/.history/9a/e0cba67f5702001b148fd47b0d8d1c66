<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<xs:schema xmlns="http://www.tibco.com/CLEver/ExtraTypes" xmlns:xs="http://www.w3.org/2001/XMLSchema" attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://www.tibco.com/CLEver/ExtraTypes">
	<xs:element name="Empty" type="xs:string"/>
	<xs:element name="ID" type="xs:integer"/>
	<xs:element name="ReturnValue" type="xs:string"/>
	<xs:element name="Security">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="UserName" type="xs:string"/>
				<xs:element name="Password" type="xs:string"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Version" type="xs:string"/>
	<xs:element name="Authentication">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Security"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Authorization">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="UserName" type="xs:string"/>
				<xs:element name="Object" type="xs:string"/>
				<xs:element name="Operation" type="xs:string"/>
				<xs:element minOccurs="0" ref="Version"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ValidateOperation">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Security"/>
				<xs:element name="Object" type="xs:string"/>
				<xs:element name="Operation" type="xs:string"/>
				<xs:element minOccurs="0" ref="Version"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="SecurityResult">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Approved" type="xs:boolean"/>
				<xs:element minOccurs="0" name="Reason" type="xs:string"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="SecurityInfo">
		<xs:complexType>
			<xs:sequence>
				<xs:element minOccurs="0" name="isCreatable" type="xs:boolean"/>
				<xs:element minOccurs="0" name="isReadable" type="xs:boolean"/>
				<xs:element minOccurs="0" name="isUpdatable" type="xs:boolean"/>
				<xs:element minOccurs="0" name="isDeletable" type="xs:boolean"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Parameters">
		<xs:complexType>
			<xs:sequence>
				<xs:element maxOccurs="unbounded" minOccurs="0" ref="Parameter"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Parameter">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Name" type="xs:string"/>
				<xs:element minOccurs="0" name="Value" type="xs:string"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="RestrictionParameters">
		<xs:complexType>
			<xs:sequence>
				<xs:element minOccurs="0" name="TransactionId" type="xs:string"/>
				<xs:element minOccurs="0" name="Service" type="xs:string"/>
				<xs:element minOccurs="0" name="Component" type="xs:string"/>
				<xs:element minOccurs="0" name="ProcedureName" type="xs:string"/>
				<xs:element minOccurs="0" name="HostName" type="xs:string"/>
				<xs:element minOccurs="0" name="ExceptionCategory" type="xs:string"/>
				<xs:element minOccurs="0" name="ExceptionCode" type="xs:string"/>
				<xs:element minOccurs="0" name="ExceptionSeverity" type="xs:string"/>
				<xs:element minOccurs="0" name="ExceptionType" type="xs:string"/>
				<xs:element minOccurs="0" name="StartDate" type="xs:int"/>
				<xs:element minOccurs="0" name="EndDate" type="xs:int"/>
				<xs:element minOccurs="0" name="TechnicalDomain" type="xs:string"/>
				<xs:element minOccurs="0" name="BusinessDomain" type="xs:string"/>
				<xs:element minOccurs="0" name="BusinessOperation" type="xs:string"/>
				<xs:element minOccurs="0" name="BusinessConcept" type="xs:string"/>
				<xs:element minOccurs="0" name="Status" type="xs:string"/>
				<xs:element minOccurs="0" name="Role" type="xs:string"/>
				<xs:element minOccurs="0" name="BusinessRef">
					<xs:complexType>
						<xs:sequence>
							<xs:element minOccurs="0" name="Name" type="xs:string"/>
							<xs:element minOccurs="0" name="Value" type="xs:string"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="ProcessContext">
		<xs:sequence>
			<xs:element minOccurs="0" name="BusinessDomain" type="xs:string"/>
			<xs:element minOccurs="0" name="TechnicalDomain" type="xs:string"/>
			<xs:element minOccurs="0" name="BusinessConcept" type="xs:string"/>
			<xs:element minOccurs="0" name="BusinessOperation" type="xs:string"/>
			<xs:element minOccurs="0" name="BusinessRefs">
				<xs:complexType>
					<xs:sequence>
						<xs:element maxOccurs="unbounded" minOccurs="0" ref="Parameter"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="TransactionID" type="xs:string"/>
			<xs:element minOccurs="0" name="ServiceName" type="xs:string"/>
			<xs:element minOccurs="0" name="ProcessName" type="xs:string"/>
			<xs:element minOccurs="0" name="ComponentName" type="xs:string"/>
			<xs:element minOccurs="0" name="HostName" type="xs:string"/>
			<xs:element minOccurs="0" name="DateTime" type="xs:dateTime"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="ProcessContext" type="ProcessContext"/>
	<xs:element name="ApplicationRole">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Roles">
					<xs:complexType>
						<xs:sequence>
							<xs:element maxOccurs="unbounded" minOccurs="0" name="Role">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Name" type="xs:string"/>
										<xs:element ref="SecurityInfo"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>
