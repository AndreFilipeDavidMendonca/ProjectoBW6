<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" 
	targetNamespace="http://www.example.org/Finalize" 
	xmlns:tns="http://www.example.org/Finalize" 
	elementFormDefault="qualified"
	xmlns:logical="http://www.project.org/Logical" >

    <import schemaLocation="Logical.xsd" namespace="http://www.project.org/Logical"/>
	
	<element name="In" type="tns:In"/>
	<element name="ProcessContext" type="tns:ProcessContext"/>
	<complexType name="In">
		<sequence>
			<element ref="logical:input" minOccurs="0"/>
			<element ref="logical:output" minOccurs="0"/>
		</sequence>
	</complexType>
	<complexType name="ProcessContext">
		<sequence>
			<element name="BusinessDomain" type="string"
				minOccurs="0" />
			<element name="TechnicalDomain" type="string"
				minOccurs="0" />
			<element name="BusinessConcept" type="string"
				minOccurs="0" />
			<element name="BusinessOperation" type="string"
				minOccurs="0" />
			<element name="TransactionID" type="string" />
			<element name="ServiceName" type="string"
				minOccurs="0" />
			<element name="ProcessName" type="string"
				minOccurs="0" />
			<element name="ComponentName" type="string"
				minOccurs="0" />
			<element name="HostName" type="string"
				minOccurs="0" />
			<element name="DateTime" type="dateTime"
				minOccurs="0" />
		</sequence>
	</complexType>
</schema>